  <Dataset>
    <ColumnName>KaBagID</ColumnName>
    <Category />
    <Subcategory />
    <Setting />
    <Parameter />
    <UserDefined />
    <MaxLength>20</MaxLength>
    <HasReference>true</HasReference>
    <Ref />
    <ParentDatabase>HRdb_PS</ParentDatabase>
    <Query>SELECT S.StaffIDNew AS KabagID, S.st_nama AS KabagName
    , CASE WHEN S.st_ter_date IS NULL THEN CONVERT(BIT, 0) ELSE CONVERT(BIT, 1) END AS IsTerminated
    , ISNULL(S.emailMM, '') AS EMail, S.GDepartmentID AS DepartmentID
    , CASE WHEN CL.Com_Loc_ID = 'SSM-KALIMANTAN' THEN 'SSM-KLMT' ELSE CL.Com_Loc_ID END AS ComLocID
    , ISNULL(GD.GDepartmentName, '') AS DepartmentName
    , ISNULL(GSD.GSubDepartmentName, '') AS SubDepartmentName
    , S.st_lk_kode AS LocationID
    , LOKP.lk_nama AS LocationName
    , S.Lokasi_Lapangan AS LocationOperationalID
    , LOKL.lk_nama AS LocationOperationalName
    , 'PERMANENT' AS EmployementStatus
    , JT.JobTitleName AS JobTitle
    , P.CompanyGroup
    , st_nomor AS StaffID_Lama
    , P.pt_nama AS CompanyName
    , Kabag_ID AS KabagID
    , S.NIK
    , S.IsBlacklist
FROM Staff S  
INNER JOIN Perusahaan P ON S.st_pt_kode = P.pt_kode
LEFT JOIN AnnualTax_MappingCompany C ON C.Pt_Kode = S.st_pt_kode COLLATE DATABASE_DEFAULT  
LEFT JOIN AnnualTax_MappingLocation L ON L.Lk_Kode = S.st_lk_kode COLLATE DATABASE_DEFAULT   
LEFT JOIN mm_com_loc CL 
ON CL.Company_ID = C.Company_ID COLLATE DATABASE_DEFAULT AND CL.Location_ID = L.Location_ID COLLATE DATABASE_DEFAULT   
LEFT JOIN GeneralDepartment GD ON GD.GDepartmentID = S.GDepartmentID  
LEFT JOIN GeneralSubDepartment GSD ON GSD.GDepartmentID = S.GDepartmentID AND GSD.GSubDepartmentID = S.GSubDepartmentID 
LEFT JOIN Lokasi LOKP ON S.st_lk_kode = LOKP.lk_kode COLLATE DATABASE_DEFAULT
LEFT JOIN Lokasi LOKL ON S.Lokasi_Lapangan = LOKL.lk_kode COLLATE DATABASE_DEFAULT
LEFT JOIN JobTitle JT ON S.JobTitleID = JT.JobTitleID COLLATE DATABASE_DEFAULT</Query>
  </Dataset>
